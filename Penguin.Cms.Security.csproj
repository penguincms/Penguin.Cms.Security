<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="Penguin.Projects.Shared\Penguin.Projects.Shared.csproj" />
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageProjectUrl>https://github.com/penguincms/Penguin.Cms.Security</PackageProjectUrl>
    <Description>Contains permissions centric classes for managing users/groups/roles in home-grown authentication systems as well as providing shared classes for permissionable entities to allow for access control</Description>
    <Version>3.3.3</Version>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="Static\**" />
    <EmbeddedResource Remove="Static\**" />
    <None Remove="Static\**" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Penguin.Cms.Attributes" Version="2.0.0" />
    <PackageReference Include="Penguin.Cms.Core" Version="1.0.3" />
    <PackageReference Include="Penguin.Cms.Entities" Version="2.0.1" />
    <PackageReference Include="Penguin.Entities" Version="2.0.1" />
    <PackageReference Include="Penguin.Extensions.String" Version="3.0.2" />
    <PackageReference Include="Penguin.Persistence.Abstractions" Version="2.1.6" />
    <PackageReference Include="Penguin.Reflection" Version="4.0.6" />
    <PackageReference Include="Penguin.Reflection.Extensions" Version="3.0.2" />
    <PackageReference Include="Penguin.Security.Abstractions" Version="1.2.0" />
  </ItemGroup>
</Project>
